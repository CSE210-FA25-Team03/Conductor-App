{
  "source": {
    "include": ["./"],                       
    "includePattern": ".+\\.js(doc|x)?$",    
    "exclude": ["node_modules", "test"]     
  },
  "opts": {
    "destination": "./docs",               
    "recurse": true,
    "encoding": "utf8",
    "template": "templates/default",
    "readme": "README.md"
  },
  "plugins": [],
  "tags": {
    "allowUnknownTags": true
  }
}
